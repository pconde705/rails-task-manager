
<%= form_for(@game) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :format %>
  <%= f.text_field :format %>
  <%= f.label :msrp %>
  <%= f.text_field :msrp %>
  <%= f.label :multiplayer %>
  <%= f.text_field :multiplayer %>
  <%= f.label :release_date %>
  <%= f.text_field :release_date %>
  <%= f.submit %>
<% end %>
